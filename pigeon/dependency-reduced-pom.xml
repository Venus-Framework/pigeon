                  <include>com.dianping:pigeon-remoting-http</include>
                  <include>com.dianping:pigeon-monitor-cat</include>
                  <include>com.dianping:pigeon-config-lion</include>
                  <include>com.dianping:pigeon-console</include>
                </includes>
              </artifactSet>
              <transformers>
                <transformer>
                  <resource>META-INF/spring.handlers</resource>
                </transformer>
                <transformer>
                  <resource>META-INF/spring.schemas</resource>
                </transformer>
                <transformer>
                  <resource>META-INF/spring.tooling</resource>
                </transformer>
                <transformer />
                <transformer />
              </transformers>
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
